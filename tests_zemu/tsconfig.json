{
  "compilerOptions": {
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "resolveJsonModule":true
  },
  "exclude": [
    "node_modules",
    "./dist/**"
  ]
}
